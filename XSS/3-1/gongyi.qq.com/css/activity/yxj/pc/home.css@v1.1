@charset "gbk";
/* CSS Document */
/*html5初始化*/
header, hgroup, menu, nav, section, menu,footer,article{display:block; margin:0; padding:0;}
body,p,input,h1,h2,h3,h4,ul,ol,li,dl,dt,dd,form{margin:0; padding:0; list-style:none; vertical-align:middle;}
.btn{
  display: inline-block;
  width: 230px;
  height: 60px;
  line-height: 60px;
  text-align: center;
  font-size: 28px;
  border-radius: 3px;
  -webkit-border-radius:3px;
  -moz-border-radius:3px;
  cursor: pointer;
  text-decoration: none;
}
.smallbtn{
  display: inline-block;
  width: 170px;
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 24px;

  border-radius: 3px;
  -webkit-border-radius:3px;
  -moz-border-radius:3px;

  cursor: pointer;
  text-decoration: none;
}
.hide{
  display: none;
}
.top_radius{
  border-top-left-radius: 4px;
  border-top-right-radius:4px;
  -webkit-border-top-left-radius: 4px;
  -webkit-border-top-right-radius: 4px;
  -moz-border-top-left-radius:4px;
  -moz-border-top-right-radius:4px;
}
.bottom_radius{
  border-bottom-left-radius: 4px;
  border-bottom-right-radius:4px;
  -webkit-border-bottom-left-radius: 4px;
  -webkit-border-bottom-right-radius: 4px;
  -moz-border-bottom-left-radius:4px;
  -moz-border-bottom-right-radius:4px;
}


.banner-wrap{width:100%; background:#efeae7; }

.home-video-wrap{width:940px; height:89px;overflow:hidden; position:relative; padding:20px 0px;}
.home-video-div{width:840px; overflow:hidden; margin-left:45px;}
.home-video-list{ list-style:none; clear:both; height:89px; display:block; float:left;}
.home-video-list li{float:left; width:158px; height:89px; margin:0 6px; position:relative;}
.home-video-list li p{display:none; margin:0; padding:0; padding:30px 10px; height:100%; width:138px; opacity: 0.7; filter: alpha(opacity=70); background:#000; color:#FFF; font-size:12px; position:absolute; z-index:10; top:0; left:0;}
/*.home-video-list li div p{}*/ 
.home-video-list li:hover{}
.home-video-wrap .h-a{width:27px; height:51px; position:absolute; display:block; background:url(http://mat1.gtimg.com/gongyi/activity/yxj/pc/home-video/v-arrow.jpg) no-repeat; top:38px;}
.home-video-wrap .h-a-pre{left:0;}
.home-video-wrap .h-a-next{right:0; background-position:0 -51px;}











