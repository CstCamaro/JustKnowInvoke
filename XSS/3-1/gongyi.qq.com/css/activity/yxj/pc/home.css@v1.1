/*banner*/
.bannerarea{ width:960px; margin:0 auto;
  position: relative;
}
#banner{
  position: relative;
}
.sideShow {
height: 532px;

width: 100%;
max-height: 532px;
background: #fff;
z-index: 1;

margin: 0 auto;
background: transparent url(http://im-img.qq.com/home/img/q2013/loading.gif) no-repeat center center;
}
.sideShow div {
position: absolute;
max-height: 532px;
width: 100%;
display: none;
}
.circle{
width: 100%;
text-align: center;
position: absolute;
bottom: 0px;
padding-bottom: 15px;
z-index: 10;
}
.circle li.on {
background: transparent url(http://mat1.gtimg.com/gongyi/activity/yxj/pc/active.png) no-repeat center center  ;
background-size: 100%;
width: 14px;
height: 14px;
}

.circle li {
display: inline-block;
 
margin: 0 2px;
width: 14px;
height: 14px;

background: transparent url(http://mat1.gtimg.com/gongyi/activity/yxj/pc/normal.png) no-repeat center center  ;
background-size: 100%;
cursor: pointer;
}

.floatarea{
  position: absolute;
  z-index: 10;
bottom: 15px;
left: 50px;
}
.floatarea .p1{
  font-size: 22px;
  color: #fff;
  padding-bottom:8px;
}
.floatarea .p2{
  font-size: 30px;
}
#total_amount{
  color: #000;
  letter-spacing: 6px;
  background: transparent url(http://mat1.gtimg.com/gongyi/activity/yxj/pc/number.png) repeat-x -1px 3px;
}
.total-amount-wrap span{ font-family:Arial; width:24px; height:32px; display:block; float:left;
  background: transparent url(http://mat1.gtimg.com/gongyi/activity/yxj/pc/number.png) repeat-x; text-align:center; line-height:32px;
}
.total-amount-wrap span.uint{background:none; font-family:"Microsoft YaHei";color: #fff;line-height:normal; font-size:22px;}
.donate{
  background-color: #c6171f;
  color: #fff;
  position: absolute;
  bottom: 70px;
  right: 80px;
  z-index: 10;
}
.donateEnd{
  background-color: #bdbdbd;
  color: #817b7b;
  position: absolute;
  bottom: 70px;
  right: 80px;
  z-index: 10;
  cursor:default;
}
.donateEnd:hover{color:#817b7b;}
.donate:hover{color:#FFF;}

