/* CSS Document */
html{color:#333333;background-color:#FFFFFF;-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%}
body,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{margin:0;padding:0}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1}
body,button,input,select,textarea{font:12px/1.5 "Î¢ÈíÑÅºÚ",tahoma,arial,sans-serif}
h1,h2,h3,h4,h5,h6{font-size:1em}
a{text-decoration:none}
a:hover{text-decoration:underline}
a:active{outline:0 none;background-color:transparent}
small,cite,code,em,th,i,b{font-size:1em;font-style:normal;font-weight:500}
input,select,textarea,button{-webkit-box-sizing:content-box;-moz-box-sizing:content-box;box-sizing:content-box}
button,input[type=button],input[type=submit]{cursor:pointer}
button[disabled],input[disabled]{cursor:default}
button::-moz-focus-inner,input::-moz-focus-inner{border:0 none;padding:0}
textarea{resize:vertical;overflow:auto;vertical-align:top}
input[type=text]:focus,textarea:focus{outline:0 none}
table{border-collapse:collapse;border-spacing:0}
fieldset,img,iframe{border:0 none}
img{display:inline-block;vertical-align:bottom;-ms-interpolation-mode:bicubic}
iframe{display:block}
ul,ol{list-style:none}
li{vertical-align:top}
q:before,q:after{content:''}
header {
  background-color: #ffffff;
  border-bottom: 1px solid #cacaca;
}
header .header {
  height: 33px;
  padding-top: 12px;
  margin: 0 auto;
  width: 980px;
}
header .header .logo {
  float: left;
  background: url("http://mat1.gtimg.com/gongyi/act/yiqijuan/logo.png");
  _background: none;
  _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="http://mat1.gtimg.com/gongyi/act/yiqijuan/logo.png");
  width: 129px;
  height: 27px;
  text-indent: -150%;
  overflow: hidden;
  cursor: pointer;
}
header .header nav {
  float: left;
  height: 27px;
}
header .header nav ul {
  background: url('http://mat1.gtimg.com/gongyi/act/yiqijuan/nav.png') no-repeat;
  float: left;
  font-size: 0;
  width: 317px;
  padding: 0 14px;
  margin-left: 37px;
  overflow: hidden;
}
header .header nav ul li {
  display: inline-block;
  height: 27px;
  line-height: 27px;
  *display: inline;
  /* IE6¡¢7 block ÔªËØ */

  *zoom: 1;
}
header .header nav ul li a {
  color: #fff;
  font-size: 14px;
  height: 100%;
  font-family: hiragino sans gb, simsun, tahoma, arial, sans-serif;
  text-decoration: none;
  display: inline-block;
  padding: 0 13px;
}
header .header nav ul li a:hover {
  background-color: #438513;
  text-decoration: none;
}
header .infor {
  float: right;display:inline-block;
 /* width: 264px;
  _width: 275px;*/
  margin-top: 7px;
  height: 13px;
  font-family: Arial,hiragino sans gb,simsun;
  vertical-align: bottom;
}
header .infor ul li {
  float: left;
  font-size: 12px;
}
header .infor ul li a {
  color: #438513;
}
header .infor ul li #signout {
  font-style: normal;
  color: #8a8a8a;
}
header .infor ul li.userinfo {
  padding-right: 7px;
}
header .infor ul li.userinfo #username {
  color: #9898b9;
  margin-right: 3px;
}
header .infor ul li.userinfo a img {
  vertical-align: middle;
}
header .infor ul li.benyue {
  padding: 0 7px;
}
header .infor ul li.selfinfor {
  padding: 0 7px;
}
.footer{ width:100%; height:108px; background:#489f23; margin-top:20px;}
.footer_info{ width:980px; height:90px; margin:0 auto;padding-top:20px;}
.footer_info a{color:#FFF;}
.footer_info a:hover{text-decoration:underline;}
.foot_info_sp1{ font-size:12px; color:#ffffff; text-align:center; line-height:40px;}
.foot_info_sp2{ font-size:12px; color:#ffffff; text-align:center; line-height:20px;}
