<script>
var _hmt = _hmt || [];
setTimeout(function(){
  function loadjs(url, callback){
      if(url == null) return ;
      var hm = document.createElement("script");
      hm.src = url;
      var s  = document.getElementsByTagName("script")[0]; 
      if(!!window.ActiveXObject || "ActiveXObject" in window){
        hm.onreadystatechange = function(){
            if ( this.readyState === 'loaded' || this.readyState === 'complete' ) {
                typeof(callback) === 'function' ? callback() : void(0);
                this.onreadystatechange = null;
            }
        };
      }else{
        hm.onload = function(){
          typeof(callback) === 'function' ? callback() : void(0);
        }
      }
      s.parentNode.insertBefore(hm, s);
    }

    loadjs('//hm.baidu.com/hm.js?e99aee90ec1b2106afe7ec3b199020a7');

    loadjs('//hm.baidu.com/hm.js?436e0af00b4308082985d33e8abe407e');

    loadjs('//hm.baidu.com/hm.js?acb0b768a874d7735d188628b12ee199');

    loadjs('//pingjs.qq.com/ping.js', function(){
      if(typeof(pgvMain) == 'function'){
        pgvMain();
      }
    });
  }, 1500)
</script>

</div>

