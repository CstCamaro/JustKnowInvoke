		     }
			var login_type = 0;			
	     	if(!!logoutMain){		      		
			      logoutMain.onclick=function(){			      
					if(login_type == 0) {
		      	    // qq退出	      	
						qqLogout();
//						window.location.href = "http://class.qq.com/";
						location.reload();
					} else { 
					//微信退出
					$.ajax({
						url:'https://class.qq.com/main/logOut',
						type:'get',
						success:function(resp){	
							qqLogout();						
//							window.location.href = "http://class.qq.com/";
							location.reload();
						}
					});

					}
			      }
		      }
		      //成为提供者
		      var cpCourse = document.getElementById('cpCourse');
		      var cplogin_close=document.getElementById('cplogin_close');
		      var cpMain = document.getElementById('cpMain');
		      var cpCancelMain = document.getElementById('cpCancelMain');
		      var cpLoginMain = document.getElementById('cpLoginMain');
		      var loginType = 0;	
		      if(!!cpCourse){
		      		cpCourse.onclick=function(){
				      	if(loginType == 1){
				      		cpMain.style.display = 'block';
				      	}else{		      					      		
	      					// cpCourse.setAttribute('target',"_blank");
//				      		window.location.href =window.location.origin+"//";
				      		window.location.href = "http://cp.class.qq.com/provider/tpl/static/login-provider.html";
				      	}
				      }
		      }	      
		      
		      
		      
		      cplogin_close.onclick = function(){
		      	cpMain.style.display = 'none';
		      }
		      cpCancelMain.onclick = function(){
		      	cpMain.style.display = "none";
		      }
		      cpLoginMain.onclick = function(){
		      	cpMain.style.display = "none";
		     //  	box.style.display = "block";
			    // maskLayer.style.display = "block";	
			    //微信退出
					$.ajax({
						url:'https://class.qq.com/main/logOut',
						type:'get',
						success:function(resp){	
							qqLogout();						
//							window.location.href = "http://class.qq.com/";
							location.reload();
						}
					});			
		      }
	      	 
	    </script>
		<script language="javascript">
		    if(typeof(pgvMain) == 'function'){
		        pgvMain();
		    }
		</script>
		
	</body>
</html>
