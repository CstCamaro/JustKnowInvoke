    <script>
    var getQuery = function(name) {
        var result = location.search.match(new RegExp("[\?\&]" + name + "=([^\&]+)", "i"));
        if (result == null || result.length < 1) {
            return "";
        }
        return result[1];
    }
    var campusApiUrl = (/(?:(.*?)manage)/.exec(window.location.href) || ['', 'http://m.campus.qq.com/'])[1] + 'api';
    var appid;
    if (/qq.com/.test(window.location.href)) {
        appid = 'wxd47f67924d99db93';
    } else {
        appid = 'wxa158511405ad7bf4';
    }
    var role = getQuery('role');
    var classele = $('.js-classification');
    if(role == 2) {
        $(classele.find("span")[0]).removeClass("current").addClass("other-class");
        $(classele.find("span")[1]).removeClass("other-class").addClass("current");
    }
    var campusUrl = (/(?:(.*?)manage)/.exec(window.location.href) || ['', 'http://m.campus.qq.com/'])[1] + 'api';
    var obj = new WxLogin({
        id: "wxC",
        appid: appid,
        scope: "snsapi_login",
        redirect_uri: campusApiUrl + "/site/loginNew",
        state: "campus_2",
        style: "",
        href: ""
    });
    </script>
