<script type="text/javascript">
    (function(){
        var publicHeader = {
            trim : function(str){
                return str.replace(/(^\s*)|(\s*$)/g,'');
            },
            checkDomain : function(){
                var domain = document.domain;
                if(domain.indexOf("qq.com") < 0){
                    throw new Error('The domain is not "qq.com"!');
                }else{
                    document.domain = "qq.com";
                }
            },
            getCookie : function (name){
                var cookie  = document.cookie;
                var cookieArr = cookie.split(';');
                for(var i=0;i<cookieArr.length;i++){
                    var cookieOne = cookieArr[i].split('=');
                    if(this.trim(cookieOne[0]) === this.trim(name)){
                        return cookieOne[1];
                    }
                }
            },
            removeCookie : function(obj){
                var expires = new Date();
                expires.setTime(expires.getTime()-1);
                if(!!this.getCookie(obj.name)){
                    document.cookie = obj.name+'=; expires='+expires+';path='+obj.path+';domain='+obj.domain;
                }
            }
        }
        var logout = document.getElementById('logout');
        var searchInput = document.getElementById('search-input');
        var searchHot = document.getElementById('search-hot');
        if(!!logout){
            logout.onclick = function(){
                publicHeader.checkDomain();
                publicHeader.removeCookie({domain:'.qq.com',name:'uin',path:'/'});
                publicHeader.removeCookie({domain:'.qq.com',name:'skey',path:'/'});
                // todo 退出兼容qq互联
                publicHeader.removeCookie({domain:'.qq.com',name:'101485576_access_token',path:'/'});
                publicHeader.removeCookie({domain:'.qq.com',name:'101485576_openid',path:'/'});
                // todo 退出兼容腾讯新闻qq互联
                publicHeader.removeCookie({domain:'.qq.com',name:'xinwen_jumpurl',path:'/'});
                publicHeader.removeCookie({domain:'.qq.com',name:'xinwen_headimgurl',path:'/'});
                publicHeader.removeCookie({domain:'.qq.com',name:'xinwen_nickname',path:'/'});
                publicHeader.removeCookie({domain:'.qq.com',name:'xinwen_openid',path:'/'});
                publicHeader.removeCookie({domain:'.qq.com',name:'xinwen_platform',path:'/'});
                window.location.reload();
            }
        }
        var mouseDown = false;
        if(!!searchInput && !!searchHot){
            searchInput.onfocus = function(){
                searchHot.style.display = 'block';
            }
            searchInput.onblur = function(e){
                if(mouseDown){
                    return;
                }
                searchHot.style.display = 'none';
            }
            searchHot.onmouseover = function(){
                searchHot.style.display = 'block';
            }
            searchHot.onmousedown = function(){
                mouseDown = true;
                searchHot.style.display = 'block';
            }
        }
    })(window)
</script>

