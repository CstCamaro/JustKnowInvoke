#loginBanner .apps-block{ margin-left:30px; margin-top:15px; height:50px;}
#loginBanner .apps-block li{ display:inline-block; *display:inline; *zoom:1;vertical-align:middle; height:50px;}
#loginBanner .apps-block li.font-box{ width:100px; font-size:12px;color:#999; *margin-left:4px;}
#loginBanner .apps-block li.font-box p{ height:12px;}


.list-block{ width:1000px; margin:10px auto 0;border:1px solid #dbdbdb; background:#FFF; overflow:hidden;}
