}

.cp_slide{
  position: relative;
  width: 980px;
  height: 320px;
  cursor: pointer;
}
.cp_fader{
  position: absolute;
  opacity:0;
}
.cp_fade{
  opacity:1;
  -webkit-transition:opacity .4s;
  -moz-transition:opacity .4s;
  transition:opacity .4s;
}
.cp_slide_btn{
  position: absolute;
  bottom: 30px;
  right: 60px;
  z-index: 999;
}
.cp_slide_btn li{
  float: left;
  display: block;
  margin-right: 5px;
  width: 12px;
  height: 12px;
  vertical-align: middle;
  cursor: pointer;
  background: url(http://mat1.gtimg.com/gongyi/2013/loveplan/lp_slide_normal.png) no-repeat;
  _background-image: none;
  _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='http://mat1.gtimg.com/gongyi/2013/loveplan/lp_slide_normal.png');
}

.cp_slide_btn .current{
  cursor: pointer;
