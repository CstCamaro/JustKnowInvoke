/* CSS Document */
body,h1,h2,h3,h4,blockquote,p,dl,dt,dd,ul,ol,li,th,td,form,fieldset,legend,button,input,textarea{margin:0;padding:0}
body,button,input,select,textarea{font:12px/1.5 'Î¢ÈíÑÅºÚ',arial,sans-serif; color:#232E45;text-align:left}
button,input,select,textarea,h1,h2,h3,h4{font-size:100%}
em,i{font-style:normal}
ol,ul{list-style:none; list-style-position:outside}
table{border-collapse:collapse;border-spacing:0}
th{ text-align:left; font-weight:400}
img,fieldset{border:0;vertical-align:middle}
a{color:#232E45;text-decoration:none;outline:none}
a:hover{color:#c00;text-decoration:none}
.ulTxt strong{font-size:16px;line-height:40px}
.ulTxt strong a{color:#458FCE;}
.fl,.mark{float:left}
.fr,.subMark{float:right}
.spanclass,.contentdiv,.hide{display:none}
.show{ display:block}
.clx:after,.ulTxt:after,.ulTxt li:after, .area:after{content:"\20";clear:both;height:0;display:block;overflow:hidden}
.clx, .ulTxt, .ulTxt li, .area{*zoom:1}
.clear{font-size:0; line-height:0; height:0; overflow:hidden; clear:both; visibility:hidden}

body{position: relative;background-color: #ebebeb;}
.top-block{ background:url(//mat1.gtimg.com/gd/local/top_bg_2.jpg) repeat-x center top; height:70px; overflow:hidden; position:relative;}
.top-block .tp-ctrl-block{width:1000px; height:70px; margin:0 auto; overflow:hidden; position:relative;}
.top-block .tp-ctrl-block .tpc-bt{ width:140px; height:40px; float:right; margin-left:10px; margin-top:15px; line-height:40px; text-align:right;}
.top-block .tp-ctrl-block .tpc-bt.fl{ float:left; width:220px;}
.top-block .tp-ctrl-block .tpc-bt.bt1{ background:url(//mat1.gtimg.com/gd/local/ctbtbg1.jpg) no-repeat;}
.top-block .tp-ctrl-block .tpc-bt.bt2{ background:url(//mat1.gtimg.com/gd/local/ctbtbg2.jpg) no-repeat;}
.top-block .tp-ctrl-block .tpc-bt span{ font-size:18px; color:#f97723; padding-right:15px;}
.top-block .tp-ctrl-block .tpc-bt:hover{ background-position:0 -40px;}

.g-nav{position: relative;height: 40px;border-top: 2px solid #333;border-bottom: 1px solid #cecece;background: #fff;box-shadow: 0 6px 10px #e4d8d8 inset,0 3px 4px #ccc; overflow:hidden;}
.g-nav .g-doc{width:1000px;margin: 0 auto;text-align: left;}
.m-nav li{float: left;height: 40px;font-size: 0;line-height: 0;}
.m-nav .nav-slt{border-bottom: 2px solid #fc4243;}
.m-nav .nav{display: block;height: 38px;overflow: hidden;line-height: 38px;font-size: 16px;color: #333;cursor: pointer;}
.m-nav li.mine{ display:none;}
.m-nav .nav:hover {color: #ee472c;}
.m-nav .nav .txt {display: block;margin-top: 12px;height: 16px;line-height: 16px;overflow: hidden;padding: 0 29px;border-left: 1px solid #dbdbdb;}
.g-navbg {height: 11px;overflow: hidden;background:url(//mat1.gtimg.com/gd/local/nav_bg.png) repeat-x;}

.slide-block{ width:1000px; height:320px; margin:10px auto; overflow:hidden; border:1px solid #dbdbdb; background:#FFF; position:relative;}
.wc-login-block{ background:url(https://res.wx.qq.com/zh_CN/htmledition/images/bg_universal17ced3.png); width:230px; height:270px; position:absolute; left:540px; top:10px; z-index:9; display:none;}
.wc-login-block img{ width:200px; height:200px; margin:15px auto 10px; display:block;}
.wc-login-block p{color: #B4B7BC; line-height:30px; text-align:center;}
.slide-block .m-banner{position:relative;float:left;width:760px;height:320px;overflow:hidden;}
.slide-block .m-banner .banner {position: absolute;}
.slide-block .m-banner .banner li {float: left;}
.slide-block .m-banner .banner li a img{ width:760px; height:320px;}
.slide-block .m-banner .banner .imgwrap {display: block;font-size: 0;line-height: 0;}
.slide-block img{border: 0;vertical-align: middle;}
.slide-block .m-banner .page {position:absolute;right:20px;bottom:20px;}
.slide-block .m-banner .page li{float:left;line-height:0;margin-left:5px;display:list-item;text-align: -webkit-match-parent;}
.slide-block .u-ico13 {filter:alpha(opacity=80);opacity:.8;display:block;width:10px;height:10px;overflow:hidden;background-position: -220px -40px;text-indent: -9999px;background: url(//mat1.gtimg.com/gd/local/sprite.png) no-repeat -9999px -9999px;}
.slide-block .m-banner .page li .u-ico13{ background-position:-220px -40px;}
.slide-block .m-banner .page li.on .u-ico13{filter: alpha(opacity=100);opacity: 1;background-position: -240px -40px;}

#loginBanner{float:left; border-left:1px solid #dbdbdb; height:320px; width:239px;}
