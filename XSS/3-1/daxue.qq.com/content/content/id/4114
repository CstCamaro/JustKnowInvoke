<script type="text/javascript">
var login = 0;
$(function(){
	// 是否已预约
	var type=2;	$.ajax({
		url: "/ajax_data/user_reserve/id/4114/type/"+type,
    dataType:"jsonp",
    jsonp:'callback',
		success:function(data){
			if(data.status===1){
				login = 1;
				if(data.reserve ===1){
					$('#order_view').html('已预约');
					$('#order_view').addClass('order_focus');
					$('#order_view').unbind();
				}else if(data.qq){
					$("#mail").val(data.qq+"@qq.com");
				}
			}
		}
	});
});
</script>
<script type="text/javascript" src="//cdn.daxue.qq.com/daxue_static/js/live/live_reserve.js"></script>
</html>